{
    "name": "scherersoftware/cake-websocket",
    "description": "Websocket plugin for CakePHP",
    "license": "MIT",
    "type": "cakephp-plugin",
    "homepage": "https://github.com/scherersoftware/cake-websocket",
    "require": {
        "php": ">=7.3.0",
        "cakephp/cakephp": ">=4.0.0",
        "react/event-loop": "^1.3",
        "react/socket": "^1.11",
        "cboden/ratchet": "^0.4.4",
        "josegonzalez/cakephp-queuesadilla": ">=0.7"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "scherersoftware/coding-standard": "^4.0.0"
    },
    "autoload": {
        "psr-4": {
            "Websocket\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Websocket\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    },
    "scripts": {
        "php-check": "vendor/bin/phpcs -p --standard=vendor/scherersoftware/coding-standard/scherersoftware ./src ./config"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
